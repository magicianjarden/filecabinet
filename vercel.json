{
  "version": 2,
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "app/api/convert/image/route.ts": {
      "maxDuration": 60
    },
    "app/api/convert/document/route.ts": {
      "maxDuration": 60
    },
    "app/api/convert/media/route.ts": {
      "maxDuration": 60
    },
    "app/api/progress/[jobId]/route.ts": {
      "maxDuration": 10
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 